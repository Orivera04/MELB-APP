@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <title>MelB - Modulo Estudiante</title>
    <link rel="Shortcut Icon" href="~/Content/favicon.ico" type="image/x-icon" />

    <!-- Boostrap, fuentes e iconos -->
    <link rel="stylesheet" href="~/Scripts/plugins/bootstrap/bootstrap.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">
    <link rel="stylesheet" href="~/Scripts/plugins/ionicons/ionicons.min.css">
    <link href="~/Scripts/plugins/fontawesome/font-awesome.css" rel="stylesheet">

    <!-- Plugins CSS -->
    <link href="~/Scripts/plugins/datatables/dataTables.bootstrap.min.css" rel="stylesheet"/>
    <link href="~/Scripts/plugins/datatables/responsive.bootstrap.min.css" rel="stylesheet">

    <link href="~/Scripts/plugins/bootstrap-select/css/bootstrap-select.min.css" rel="stylesheet" />
    <link href="~/Scripts/plugins/SweetAlert/sweetalert2.min.css" rel="stylesheet">
    <link href="~/Scripts/plugins/DataDrop/datedropper.min.css" rel="stylesheet">
    <link href="~/Content/SistemaEstilos.css" rel="stylesheet" />
</head>
<body>
    <!--Contenedor Global-->
    <div class="wrapper">
        <!-- Contenedor SideBar-->
        <div class="contentsidebar">
            <!-- Sidebar Holder -->
            <nav id="sidebar" class="active">
                <div class="sidebar-header">
                    <h3>Musica en los barrios</h3>
                    <strong>MelB</strong>
                </div>
                <ul class="list-unstyled components">
                    <li>
                        <a href="javascript:void(0)" id="PerfilSubMenu">
                            <i class="glyphicon glyphicon-user"></i>
                            Mi perfil
                        </a>
                        <a href="javascript:void(0)" id="NotasSubMenu">
                            <i class="glyphicon glyphicon-education"></i> Notas
                        </a>
                        <a href="javascript:void(0)" id="MatriculaSubMenu">
                            <i class="glyphicon glyphicon-book"></i> Matricula
                        </a>
                        <a href="javascript:void(0)" id="HorarioSubMenu">
                            <i class="glyphicon glyphicon-th-list"></i> Horario
                        </a>
                    </li>
                </ul>
            </nav>
        </div>


        <!-- Page Content Holder - Contenedor NavBar-->
        <div id="content">
            <nav class="navbar navbar-default" id="cabezera_header">
                <div class="container-fluid">

                    <div class="navbar-header" id="Header_Nav">
                        <button type="button" id="sidebarCollapse" class="btn btn-info navbar-btn">
                            <i class="ion-navicon-round" data-pack="default" data-tags="menu, hamburger, slide menu"></i>
                        </button>
                        <h3 class="TextNavHeader">Modulo Estudiantil</h3>
                    </div>

                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav navbar-right" id="Form_Right">
                            <li>
                                <form class="navbar-form navbar-left" role="search">
                                    <button data-toggle="tooltip" title="Actualizar datos" id="Actualizar" type="button" class="btn waves-effect waves-light btn-primary btn-color" onclick="">
                                        <i class="ion-loop" data-pack="default" data-tags="refresh"></i>
                                    </button>
                                </form>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <div>
                <div id="Alumno_Detalle">
                    <div class="row RowEstudiante">
                        <div class="panel panel-color panel-danger ">
                            <div class="panel-heading">
                                <i class="icon ion-person" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Mi perfil</h3>
                                <div class="FlotarDerecha">
                                    <i class="ion-grid" data-pack="default" data-tags="menu"></i>
                                    <div class="onoffswitch">
                                        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="Switch_Editar">
                                        <label class="onoffswitch-label" for="Switch_Editar">
                                            <span class="onoffswitch-inner"></span>
                                            <span class="onoffswitch-switch"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-3 ">
                                        <img id="ImagenEstudiante" class="img-thumbnail" src="https://i.imgur.com/YHELGVr.png">
                                    </div>

                                    <div class="col-sm-9 ">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="p-20">
                                                    <form>
                                                        <div class="form-group">
                                                            <label class="control-label">N° Estudiante</label>
                                                            <div class="form-group">
                                                                <input id="ID_Estudiante" type="text" value="@Session["ID"]" name="ID_Estudiante" data-bts-min="1" data-bts-max="2000000" data-bts-mousewheel="true" data-bts-button-down-class="btn waves-effect waves-light btn-primary btn-color" data-bts-button-up-class="btn waves-effect waves-light btn-primary btn-color" disabled />
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Apellido</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Apellido_Estudiante" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-person"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Fecha Nacimiento</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="FechaNacimiento_Estudiante" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-calendar"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Apellido del tutor</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Tutor_EstudianteApellido" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-person-stalker"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Telefono #1</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="EstudianteTelefono1" type="text" class="form-control" disabled maxlength="8">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="ion-social-whatsapp"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label">Dirección</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="EstudianteDireccion" type="text" class="form-control" disabled maxlength="100">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-home"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Rol</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="EstudianteRol" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-hammer"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                    </form>
                                                </div>
                                            </div>

                                            <div class="col-md-6">
                                                <div class="p-20">
                                                    <form>
                                                        <div class="form-group">
                                                            <label class="control-label">Nombre</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Nombre_Estudiante" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-person"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Correo electronico</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Correo_Estudiante" type="text" class="form-control" disabled maxlength="25">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-at"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Nombre del tutor</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Tutor_EstudianteNombre" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-person-stalker"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Cedula</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="EstudianteCedula" type="text" class="form-control" disabled maxlength="14">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-document-text"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Telefono #2</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="EstudianteTelefono2" type="text" class="form-control" maxlength="8" disabled >
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="ion-social-whatsapp"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Sexo</label>
                                                            <div class="input-group m-t-10">
                                                                <select id="SexoEstudiante" class="selectpicker" data-style="btn-white" disabled>
                                                                    <option>M</option>
                                                                    <option>F</option>
                                                                </select>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Año de ingreso</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="EstudianteIngreso" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-clock"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12 text-center" id="BotonDatosAlumno">
                                            <button id="ActualizarAlumno" type="button" class="btn btn-success">
                                                <span class="btn-label"><i class="ion-upload active" data-pack="default" data-tags="storage, cloud"></i></span>Actualizar datos
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="panel panel-color panel-danger" id="Panel_Inscripciones">
                            <div class="panel-heading Accesorios">
                                <i class="ion-music-note" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Cursos Inscritos</h3>
                            </div>

                            <div class="panel-body ExtraPadding">
                                <div class="row">
                                    <div class="table-responsive">

                                        <table id="Inscripciones_T" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">                                            
                                            <thead>
                                                <tr>
                                                    <th>Numero de inscripción</th>
                                                    <th>Nombre del curso</th>
                                                    <th>Estado</th>
                                                </tr>
                                            </thead>
                                            <tbody id="Inscripciones_TBody"></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="Nota_Detalle">
                    <div class="row RowEstudiante">

                        <div class="panel panel-color panel-danger">
                            <div class="panel-heading" id="Panel_Filtro_Cabezera">
                                <i class="icon ion-clipboard" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Notas</h3>
                            </div>
                            <div class="panel-body">
                                <div class="row" id="Row_Responsive_Notas">
                                    <div class="col-md-3" align="center">
                                        <div class="form-inline">
                                            <div class="input-group m-t-10">
                                                <label class="control-label col-md-3" id="Curso_Res" align="center">Curso</label>
                                                <select id="Curso_Alumno" class="selectpicker" data-style="btn-white" data-width="100%"></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-3" align="center">
                                        <div class="form-inline">
                                            <div class="input-group m-t-10">
                                                <label class="control-label col-md-3" id="Nivel_Res" align="center">Año</label>
                                                <select id="Año_Alumno" class="selectpicker" data-style="btn-white" data-width="100%">
                                                    <option>2018</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-3" align="center">
                                        <div class="form-inline">
                                            <div class="input-group m-t-10">
                                                <label class="control-label col-md-3" id="SemestreRes" align="center">Semestre</label>
                                                <select id="Semestre" class="selectpicker" data-style="btn-white" data-width="100%">
                                                    <option>I</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3" align="center">
                                        <button id="NotasBoton" type="button" class="btn waves-effect waves-light btn-filtro" style="display: inline-block;"><span class="btn-label"><i class="ion-funnel" data-pack="default" data-tags="add, include, new, invite, +"></i></span> Filtrar datos</button>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

                <div id="Matricula_Detalle">
                    <div class="row RowEstudiante">

                        <div class="panel panel-color panel-danger">
                            <div class="panel-heading" id="Panel_Filtro_Cabezera">
                                <i class="icon ion-document-text" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Matriculas</h3>
                            </div>
                            <div class="panel-body">
                                <div class="row" id="Row_Responsive_Notas">
                                    <div class="col-md-3" align="center">
                                        <div class="form-inline">
                                            <div class="input-group m-t-10">
                                                <label class="control-label col-md-3" id="Curso_ResMat" align="center">Curso</label>
                                                <select id="Curso_Alumno" class="selectpicker" data-style="btn-white" data-width="100%"></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-3" align="center">
                                        <div class="form-inline">
                                            <div class="input-group m-t-10">
                                                <label class="control-label col-md-3" id="Nivel_ResMat" align="center">Año</label>
                                                <select id="Año_Alumno" class="selectpicker" data-style="btn-white" data-width="100%">
                                                    <option>2018</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-3" align="center">
                                        <div class="form-inline">
                                            <div class="input-group m-t-10">
                                                <label class="control-label col-md-3" id="SemestreResMat" align="center">Semestre</label>
                                                <select id="Semestre" class="selectpicker" data-style="btn-white" data-width="100%">
                                                    <option>I</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3" align="center">
                                        <button id="NotasBoton" type="button" class="btn waves-effect waves-light btn-filtro" style="display: inline-block;"><span class="btn-label"><i class="ion-funnel" data-pack="default" data-tags="add, include, new, invite, +"></i></span> Filtrar datos</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="Horario_Detalle">
                    <div class="row RowEstudiante">

                    </div>
                </div>

            </div>

        </div>
    </div>

    <script src="~/Scripts/plugins/jquery/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/plugins/jquery-ui/jquery-ui.min.js"></script>

    <!-- Plugins Boostrap -->
    <script src="~/Scripts/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/plugins/datatables/dataTables.responsive.min.js"></script>
    <script src="~/Scripts/plugins/datatables/responsive.bootstrap.min.js"></script>

    <script src="~/Scripts/plugins/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/Scripts/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>
    <script src="~/Scripts/plugins/bootstrap-touchspin/js/jquery.bootstrap-touchspin.min.js"></script>
    <script src="~/Scripts/plugins/DataDrop/datedropper.min.js" type="text/javascript"></script>

    <!-- Plugins Alertas -->
    <script src="~/Scripts/plugins\SweetAlert/sweetalert2.min.js"></script>
    <!-- Cargando Boostrap y sus dependencias -->
    <script src="~/Scripts/bootstrap.min.js"></script>

    <!-- Scripts de MeLB -->
    <script src="~/Scripts/EventosNavegadorEstudiante.js"></script>

    <!-- Scripts Web API-->
    <script src="~/Scripts/webapi/Estudiante.js"></script>
    <script src="~/Scripts/webapi/Inscripciones.js"></script>

</body>
</html>