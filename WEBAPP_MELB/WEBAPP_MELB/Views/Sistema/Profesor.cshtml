
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <title>MelB - Modulo Profesor</title>
    <link rel="Shortcut Icon" href="~/Content/favicon.ico" type="image/x-icon" />

    <!-- Boostrap, fuentes e iconos -->
    <link rel="stylesheet" href="~/Scripts/plugins/bootstrap/bootstrap.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">
    <link rel="stylesheet" href="~/Scripts/plugins/ionicons/ionicons.min.css">
    <link href="~/Scripts/plugins/fontawesome/font-awesome.css" rel="stylesheet">

    <!-- Plugins CSS -->
    <link href="~/Scripts/plugins/datatables/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Scripts/plugins/datatables/responsive.bootstrap.min.css" rel="stylesheet">

    <link href="~/Scripts/plugins/bootstrap-select/css/bootstrap-select.min.css" rel="stylesheet" />
    <link href="~/Scripts/plugins/SweetAlert/sweetalert2.min.css" rel="stylesheet">
    <link href="~/Scripts/plugins/DataDrop/datedropper.min.css" rel="stylesheet">
    <link href="~/Content/SistemaEstilos.css" rel="stylesheet" />
</head>
<body>
    <!--Contenedor Global-->
    <div class="wrapper">
        <!-- Contenedor SideBar-->
        <div class="contentsidebar">
            <!-- Sidebar Holder -->
            <nav id="sidebar" class="active">
                <div class="sidebar-header">
                    <h3>Musica en los barrios</h3>
                    <strong>MelB</strong>
                </div>
                <ul class="list-unstyled components">
                    <li>
                        <a href="javascript:void(0)" id="EstadisticaProfesorSubMenu">
                            <i class="glyphicon glyphicon-home"></i> Inicio
                        </a>
                        <a href="javascript:void(0)" id="CursosProfesorSubMenu">
                            <i class="glyphicon glyphicon-list-alt"></i> Cursos
                        </a>
                        <a href="javascript:void(0)" id="HorarioProfesorSubMenu">
                            <i class="glyphicon glyphicon-th-list"></i> Horario
                        </a>
                        <a href="javascript:void(0)" id="PerfilProfesorSubMenu">
                            <i class="glyphicon glyphicon-user"></i>
                            Perfil
                        </a>
                    </li>
                </ul>
            </nav>
        </div>

        <!-- Page Content Holder - Contenedor NavBar-->
        <div id="content">
            <nav class="navbar navbar-default" id="cabezera_header">
                <div class="container-fluid">

                    <div class="navbar-header" id="Header_Nav">
                        <button type="button" id="sidebarCollapse" class="btn btn-info navbar-btn">
                            <i class="ion-navicon-round" data-pack="default" data-tags="menu, hamburger, slide menu"></i>
                        </button>
                        <h3 class="TextNavHeader">Modulo Profesor</h3>
                    </div>

                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav navbar-right" id="Form_Right">
                            <li>
                                <form class="navbar-form navbar-left" role="search">
                                    <button data-toggle="tooltip" data-placement="bottom" title="General reporte en pdf" id="ReporteProfesor" type="button" class="btn waves-effect waves-light btn-primary btn-color" onclick="">
                                        <i class="icon ion-android-download" data-pack="default" data-tags="refresh"></i>
                                    </button>
                                </form>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>

            <div>

                <div id="Profesor_Detalle">
                    <div class="row RowProfesor">
                        <div class="panel panel-color panel-danger ">
                            <div class="panel-heading">
                                <i class="icon ion-person" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Mi perfil</h3>
                                <div class="FlotarDerecha">
                                    <i class="ion-grid" data-pack="default" data-tags="menu"></i>
                                    <div class="onoffswitch">
                                        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="Switch_Editar_Profesor">
                                        <label class="onoffswitch-label" for="Switch_Editar_Profesor">
                                            <span class="onoffswitch-inner"></span>
                                            <span class="onoffswitch-switch"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-3">
                                        <img id="Imagen_Profesor" class="img-thumbnail">
                                    </div>
                                    <div class="col-sm-9 ">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="p-20">
                                                    <form>
                                                        <div class="form-group">
                                                            <label class="control-label">ID Empleado</label>
                                                            <div class="form-group">
                                                                <input id="ID_Profesor" type="text" value="@Session["ID"]" name="ID_Profesor" data-bts-min="1" data-bts-max="2000000" data-bts-mousewheel="true" data-bts-button-down-class="btn waves-effect waves-light btn-primary btn-color" data-bts-button-up-class="btn waves-effect waves-light btn-primary btn-color" disabled />
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Nombre</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Nombre_Profesor" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-person"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Correo electronico</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Correo_Profesor" type="text" class="form-control" disabled maxlength="25">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-at"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Telefono #1</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="ProfesorTelefono1" type="text" class="form-control" disabled maxlength="8">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="ion-social-whatsapp"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Sexo</label>
                                                            <div class="input-group m-t-10">
                                                                <select id="SexoProfesor" class="selectpicker" data-style="btn-white" disabled>
                                                                    <option>M</option>
                                                                    <option>F</option>
                                                                </select>
                                                            </div>
                                                        </div>

                                                    </form>
                                                </div>
                                            </div>

                                            <div class="col-md-6">
                                                <div class="p-20">
                                                    <form>

                                                        <div class="form-group">
                                                            <label class="control-label">Cedula</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="ProfesorCedula" type="text" class="form-control" disabled maxlength="14">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-document-text"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Apellido</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Apellido_Profesor" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-person"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Dirección</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="ProfesorDireccion" type="text" class="form-control" disabled maxlength="100">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-home"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Telefono #2</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="ProfesorTelefono2" type="text" class="form-control" maxlength="8" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="ion-social-whatsapp"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Cargo</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="ProfesorCargo" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-android-sunny"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                    </form>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12 text-center" id="BotonDatosProfesor">
                                            <button id="ActualizarProfesor" type="button" class="btn btn-success">
                                                <span class="btn-label"><i class="ion-upload active" data-pack="default" data-tags="storage, cloud"></i></span>Actualizar datos
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

                <div id="Cursos_Profesor">
                    <div class="row RowProfesor">

                        <div class="panel panel-color panel-danger">
                            <div class="panel-heading2">
                                <i class="icon ion-android-clipboard" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Listado de Cursos</h3>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <table id="CursosProfesor_T" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>Codigo</th>
                                                <th>Nombre</th>
                                                <th>N° Alumnos</th>
                                                <th>Nivel</th>
                                                <th>Modalidad</th>
                                                <th>Estado</th>
                                                <th>Detalle</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="Horario_DetalleProfesor">
                    <div class="row RowProfesor">
                        <div class="panel panel-color panel-danger">
                            <div class="panel-heading4">
                                <i class="icon ion-erlenmeyer-flask" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Horario de clases</h3>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <table id="HorarioProfesor_T" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>Hora</th>
                                                <th>Lunes</th>
                                                <th>Martes</th>
                                                <th>Miercoles</th>
                                                <th>Jueves</th>
                                                <th>Viernes</th>
                                                <th>Sabado</th>
                                                <th>Domingo</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="Curso_DetalleProfesor">
                    <div class="row RowProfesor">
                        <div class="panel panel-color panel-danger ">
                            <div class="panel-heading5">
                                <i class="icon ion-document" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Detalle Curso</h3>
                            </div>

                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="p-20">
                                                    <form>

                                                        <div class="form-group">
                                                            <label class="control-label">Codigo Curso</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="CodigoCurso" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-pound"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Alumnos inscritos</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="NAlumnos" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-person"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Modalidad</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Modalidad" type="text" class="form-control" disabled="">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-clock"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>


                                                    </form>
                                                </div>
                                            </div>

                                            <div class="col-md-6">
                                                <div class="p-20">
                                                    <form>
                                                        <div class="form-group">
                                                            <label class="control-label">Asignatura</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Nombre_Curso" type="text" class="form-control" disabled maxlength="14">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-pricetag"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                        <div class="form-group">
                                                            <label class="control-label">Nivel</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Nivel_Curso" type="text" class="form-control" disabled maxlength="14">
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="ion-ios-lightbulb"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="control-label">Estado</label>
                                                            <div class="input-group m-t-10">
                                                                <input id="Estado_Curso" type="text" class="form-control" disabled>
                                                                <span class="input-group-btn">
                                                                    <button type="button" class="btn waves-effect waves-light btn-primary btn-color"><i class="icon ion-ios-star"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>

                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="panel panel-color panel-danger"">
                            <div class="panel-heading Accesorios">
                                <i class="ion-cube" aria-hidden="true"></i>
                                <h3 class="TextoHeader">Alumnos Inscritos</h3>
                               <div class="FlotarDerecha">
                                    <i class="ion-grid" data-pack="default" data-tags="menu"></i>
                                    <div class="onoffswitch">
                                        <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox" id="Switch_Editar_Estuche">
                                        <label class="onoffswitch-label" for="SwtichNotasPR">
                                            <span class="onoffswitch-inner"></span>
                                            <span class="onoffswitch-switch"></span>
                                        </label>
                                    </div>
                                   </div>
                            </div>

                            <div class="panel-body ExtraPadding">
                                <div class="row">
                                    <table id="Notas_T" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>ID Estudiante</th>
                                                <th>Nombres</th>
                                                <th>Apellidos</th>
                                                <th>IP</th>
                                                <th>IIP</th>
                                                <th>NF</th>
                                            </tr>
                                        </thead>
                                        <tbody id="Notas_TBody"></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12 text-center">
                            <button id="SubirNota" type="button" class="btn btn-success" disabled>
                                <span class="btn-label"><i class="ion-upload active" data-pack="default" data-tags="storage, cloud"></i></span>Subir Notas
                            </button>
                        </div>
                    </div>
                </div>

                <div id="Estadistica_Profesor">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="col-sm-5 TarjetaIzquierda DegInstrumento">
                                <i class="icon ion-coffee IconoDashboard"></i>
                            </div>
                            <div class="col-sm-7 Tarjeta">
                                <h3 class="ValorTextoDashboard Instrumento_Label" id="CantidadMaterias">0</h3>
                                <h3 class="TipoEtiqueta">Materias impartidas</h3>
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="col-sm-5 TarjetaIzquierda DegProveedores">
                                <i class="icon ion-person-stalker IconoDashboard"></i>
                            </div>
                            <div class="col-sm-7 Tarjeta">
                                <h3 class="ValorTextoDashboard Proveedor_Label" id="CantidadAlumnos">0</h3>
                                <h3 class="TipoEtiqueta">Alumnos registrados</h3>
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="col-sm-5 TarjetaIzquierda DegAccesorios">
                                <i class="icon ion-university IconoDashboard"></i>
                            </div>
                            <div class="col-sm-7 Tarjeta">
                                <h3 class="ValorTextoDashboard Accesorios_Label" id="CantidadCursos">0</h3>
                                <h3 class="TipoEtiqueta">Cursos Impartidos</h3>
                            </div>
                        </div>

                    </div>


                    <div class="row">
                        <div class="col-md-12">
                            <div class="TarjetaDashboard">

                                <div class="header">
                                    <h4 class="title">Alumnos de Cada Curso</h4>
                                    <p class="category">Grafica</p>
                                </div>
                                <div class="content">
                                    <canvas id="AlumnosCursoGrafica" style="height:230px;width: 590px;"></canvas>
                                    <div class="footer">
                                        <div class="Leyenda">
                                            <i class="fa fa-circle Leyenda1 LeyendaIcono"></i> Flauta Dulce
                                            <i class="fa fa-circle Leyenda2 LeyendaIcono"></i> Flauta Traversa
                                            <i class="fa fa-circle Leyenda3 LeyendaIcono"></i> Clarinete
                                            <i class="fa fa-circle Leyenda4 LeyendaIcono"></i> Violin
                                            <i class="fa fa-circle Leyenda5 LeyendaIcono"></i> Viola
                                            <i class="fa fa-circle Leyenda6 LeyendaIcono"></i> Cello
                                            <i class="fa fa-circle Leyenda7 LeyendaIcono"></i> Guitarra
                                            <i class="fa fa-circle Leyenda1 LeyendaIcono"></i> Piano
                                        </div>
                                        <hr>
                                        <div class="Estadisticas">
                                            <i class="fa fa-area-chart"></i>Cantidad de alumnos de los cursos que imparte el profesor
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>


    </div>


    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/plugins/jquery-ui/jquery-ui.min.js"></script>

    <!-- Plugins Boostrap -->
    <script src="~/Scripts/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/plugins/datatables/dataTables.responsive.min.js"></script>
    <script src="~/Scripts/plugins/datatables/responsive.bootstrap.min.js"></script>

    <script src="~/Scripts/plugins/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/Scripts/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>
    <script src="~/Scripts/plugins/bootstrap-touchspin/js/jquery.bootstrap-touchspin.min.js"></script>
    <script src="~/Scripts/plugins/DataDrop/datedropper.min.js" type="text/javascript"></script>
    <script src="~/Scripts/plugins/jspdf/jspdf.min.js"></script>
    <script src="~/Scripts/plugins/jspdf/jspdf.plugin.autotable.js"></script>

    <script src="~/Scripts/plugins/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/Scripts/plugins/bootstrap-maxlength/bootstrap-maxlength.min.js"></script>
    <script src="~/Scripts/plugins/bootstrap-touchspin/js/jquery.bootstrap-touchspin.min.js"></script>
    <script src="~/Scripts/plugins/DataDrop/datedropper.min.js" type="text/javascript"></script>
    <script src="~/Scripts/plugins/chartjs/Chart.min.js"></script>
    <script src="~/Scripts/plugins/jspdf/jspdf.min.js"></script>
    <script src="~/Scripts/plugins/jspdf/jspdf.plugin.autotable.js"></script>


    <!-- Plugins Alertas -->
    <script src="~/Scripts/plugins\SweetAlert/sweetalert2.min.js"></script>
    <!-- Cargando Boostrap y sus dependencias -->
    <script src="~/Scripts/bootstrap.min.js"></script>

    <!-- Scripts de MeLB -->
    <script src="~/Scripts/EventosNavegadorProfesor.js"></script>

    <!-- Scripts Web API-->
    <script src="~/Scripts/webapi/Empleado.js"></script>
    <script src="~/Scripts/webapi/Horario.js"></script>
    <script src="~/Scripts/webapi/Asignaturas.js"></script>
</body>
</html>
